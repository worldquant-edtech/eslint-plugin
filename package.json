{
  "name": "@wqlearning/eslint-plugin",
  "version": "1.2.4",
  "description": "Common ESLint plugin for WQ Learning projects.",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "scripts/build",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./src/index.js",
  "exports": {
    "import": "./src/index.js",
    "require": "./dist/cjs/index.js"
  },
  "dependencies": {
    "@eslint/js": "^9.33.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-exports": "^1.0.0-beta.5",
    "eslint-import-resolver-vite": "^2.1.0",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-mdx": "^3.6.2",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0"
  },
  "peerDependencies": {
    "eslint": ">=9"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.7",
    "@babel/preset-env": "^7.26.7",
    "@wqlearning/prettier-config": "^1.1.2",
    "eslint": "^9.33.0"
  },
  "prettier": "@wqlearning/prettier-config",
  "volta": {
    "node": "20.12.2",
    "yarn": "1.22.22"
  }
}
